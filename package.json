    "postinstall": "quasar prepare",
    "precommit-checks": "eslint . --fix && echo 'Linting complete!'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "husky": "^9.1.7",
